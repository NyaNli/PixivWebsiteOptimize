{
    "name": "Pixiv优化",
    "version": "2020",
    "manifest_version": 2,
    "content_scripts":[
        {
            "matches":[
                "*://www.pixiv.net/*"
            ],
            "js":["imgpatch.js"],
            "run_at": "document_start"
        }
    ],
    "permissions":
    [
        "*://www.pixiv.net/*"
    ]
}